  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build:local": "webpack --env NODE_ENV=local --mode development",
    "start:local": "webpack serve --env NODE_ENV=local --mode development",
    "build:dev": "webpack --env NODE_ENV=dev --mode development",
    "start:dev": "webpack serve --env NODE_ENV=dev --mode development",
    "build:prod": "webpack --env NODE_ENV=prod --mode production",
    "start:prod": "webpack serve --env NODE_ENV=prod --mode production"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|png)$": "<rootDir>/empty-module.ts"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFilesAfterEnv": [
      "./setupTests.ts"
    ]
  },
